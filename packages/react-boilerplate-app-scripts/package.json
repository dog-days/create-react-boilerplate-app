{
  "name": "react-boilerplate-app-scripts",
  "version": "0.2.10",
  "description": "react-boilerplate-app-scripts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dog-days/create-react-boilerplate-app.git"
  },
  "bugs": {
    "url": "https://github.com/dog-days/create-react-boilerplate-app/issues"
  },
  "homepage": "https://github.com/dog-days/create-react-boilerplate-app",
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "bin": {
    "react-boilerplate-app-scripts": "./bin/react-boilerplate-app-scripts.js"
  },
  "scripts": {
    "start":
      "npm run build-dll hide-message && node ./bin/react-boilerplate-app-scripts.js start",
    "build":
      "npm run build-dll hide-message && node ./bin/react-boilerplate-app-scripts.js build",
    "build-dll": "node ./bin/react-boilerplate-app-scripts.js build-dll",
    "cover": "node ./bin/react-boilerplate-app-scripts.js cover",
    "use": "node ./bin/react-boilerplate-app-scripts.js use",
    "serve-build": "node ./bin/react-boilerplate-app-scripts.js serve-build"
  },
  "keywords": ["react", "react-boilerplate-app-scripts"],
  "author": "xianshannan",
  "license": "ISC",
  "dependencies": {
    "archiver": "^2.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "^1.1.3",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "css-loader": "^0.28.1",
    "detect-port": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^3.0.0-rc.1",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.0",
    "gzip-size": "^3.0.0",
    "happypack": "^4.0.0-beta.5",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "new-serve": "^6.4.3",
    "node-xlsx": "^0.7.4",
    "object.assign": "^4.0.4",
    "postcss-loader": "^2.0.5",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-boilerplate-app-utils": "^0.0.23",
    "react-dev-utils": "^0.5.2",
    "react-hot-loader": "3.0.0-beta.6",
    "relative": "^3.0.2",
    "socket.io": "^2.0.2",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.5",
    "webpack-parallel-uglify-plugin": "^1.0.0",
    "webpack-simple-progress-plugin": "0.0.2"
  },
  "devDependencies": {
    "mvc-react": "0.1.2",
    "object.assign": "^4.0.4",
    "promise": "^7.1.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-controller-cli": "0.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "react-boilerplate-app-scripts": {
    "dll": ["react", "react-dom", "prop-types"],
    "appSrcPath": "template/mvc-react",
    "appPublicPath": "template/public",
    "mock": {
      "/common-api/([^?#]*)": "/mock/$1.json"
    }
  },
  "babel": {
    "presets": [
      "react",
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "plugins": ["transform-decorators-legacy", "transform-runtime"],
    "env": {
      "development": {
        "plugins": ["react-hot-loader/babel"]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
