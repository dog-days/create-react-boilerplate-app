{
  "name": "react-boilerplate-app-scripts",
  "version": "0.2.12-beta.1",
  "description": "react-boilerplate-app-scripts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dog-days/create-react-boilerplate-app.git"
  },
  "bugs": {
    "url": "https://github.com/dog-days/create-react-boilerplate-app/issues"
  },
  "homepage": "https://github.com/dog-days/create-react-boilerplate-app",
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "bin": {
    "react-boilerplate-app-scripts": "./bin/react-boilerplate-app-scripts.js"
  },
  "scripts": {
    "test": "karma start",
    "start":
      "npm run build-dll hide-message && node ./bin/react-boilerplate-app-scripts.js start",
    "build":
      "npm run build-dll hide-message && node ./bin/react-boilerplate-app-scripts.js build",
    "build-dll": "node ./bin/react-boilerplate-app-scripts.js build-dll",
    "cover": "node ./bin/react-boilerplate-app-scripts.js cover",
    "use": "node ./bin/react-boilerplate-app-scripts.js use",
    "serve-build": "node ./bin/react-boilerplate-app-scripts.js serve-build"
  },
  "keywords": ["react", "react-boilerplate-app-scripts"],
  "author": "xianshannan",
  "license": "ISC",
  "dependencies": {
    "archiver": "2.1.1",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chai": "4.1.2",
    "chalk": "2.4.1",
    "cli-table": "0.3.1",
    "commander": "2.15.1",
    "css-loader": "0.28.1",
    "detect-port": "1.2.3",
    "eslint": "4.1.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.34.1",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "fs-extra": "6.0.1",
    "gzip-size": "4.1.0",
    "html-webpack-plugin": "4.0.0-alpha",
    "istanbul-instrumenter-loader": "3.0.0",
    "karma": "2.0.3",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-firefox-launcher": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon": "1.0.5",
    "karma-sinon-chai": "1.3.4",
    "karma-webpack": "3.0.0",
    "mocha": "5.2.0",
    "new-serve": "6.4.3",
    "node-xlsx": "0.7.4",
    "object.assign": "4.1.0",
    "postcss-loader": "2.1.5",
    "progress-bar-webpack-plugin": "1.11.0",
    "promise": "8.0.1",
    "react-boilerplate-app-utils": "^0.0.25-beta.1",
    "react-dev-utils": "5.0.1",
    "react-hot-loader": "3.0.0-beta.6",
    "relative": "3.0.2",
    "sinon": "6.0.0",
    "sinon-chai": "3.2.0",
    "socket.io": "2.0.2",
    "style-loader": "0.21.0",
    "ts-loader": "4.4.1",
    "typescript": "2.9.2",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-dev-server": "3.1.4",
    "webpack-parallel-uglify-plugin": "1.1.0",
    "webpack-simple-progress-plugin": "0.0.4"
  },
  "devDependencies": {
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "lodash": "^4.17.4",
    "mvc-react": "0.1.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-controller-cli": "0.0.1"
  },
  "react-boilerplate-app-scripts": {
    "dll": ["react", "react-dom", "prop-types"],
    "appSrcPath": "template/simple-ts/src",
    "appPublicPath": "template/simple-ts/public",
    "mock": {
      "/common-api/([^?#]*)": "/mock/$1.json"
    },
    "typescript": true
  },
  "babel": {
    "env": {
      "development": {
        "presets": ["react", "env", "stage-0"],
        "plugins": [
          "transform-decorators-legacy",
          "transform-runtime",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": ["react", "env", "stage-0"],
        "plugins": ["transform-decorators-legacy", "transform-runtime"]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
